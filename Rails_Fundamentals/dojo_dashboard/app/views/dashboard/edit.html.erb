<h1>Edit <%= @dojo.branch %></h1>
<br>
<form action="/update/<%= @dojo.id %>" method="post" >
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type="hidden" name="_method" value="patch">
  <input type="string" name="branch" value="<%= @dojo.branch %>"><br />
  <input type="string" name="street" value="<%= @dojo.street %>"><br />
  <input type="string" name="city" value="<%= @dojo.city %>"><br />
  <input type="string" name="state" value="<%= @dojo.state %>"><br />
  <br>
  <input type='submit' value='Update'>
</form>
<br>
<a href="/">Go Back</a>
